{
    "name": "techlink/blog",
    "description": "A blog powered by laravel package.",
    "license": "MIT",
    "authors": [
        {
            "name": "Dipesh Shrestha",
            "email": "team7hookage@gmail.com"
        }
    ],
    "require": {},
    "require-dev": {
        "orchestra/testbench": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Techlink\\Blog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Techlink\\Blog\\Tests\\": "tests/",
            "Techlink\\Blog\\Database\\Factories\\" : "database/factories"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Techlink\\Blog\\Providers\\BlogProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-f": "phpunit --filter"
    }
}
